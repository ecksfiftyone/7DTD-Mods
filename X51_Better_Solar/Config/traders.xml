<configs>
  <!-- More Solar Cells -->	
    <set xpath="traders/trader_item_groups/trader_item_group[@name='traderElectrical']/item[@name='solarCell']/@prob">0.5</set> <!-- Default prob: 0.2 -->

  <!-- Materials For Solar Crafting -->		 
	<append xpath="/traders/trader_item_groups/trader_item_group[@name='generalResources']">
		<item name="resourceSilverNugget" count="1,10" prob="0.5"/>
		<item name="resourceGoldNugget" count="1,10" prob="0.5"/>
		<item name="resourceRawDiamond" count="1,10" prob="0.5"/>
	</append>

	<append xpath="/traders/trader_item_groups/trader_item_group[@name='traderElectrical']">
		<item name="solarFrame" count="1,5" prob="0.6"/>
	</append>	 

</configs>
