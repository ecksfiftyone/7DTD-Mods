<blocks>
<!--voxelblock-->
	
<!--Farm Plot Voxel Blocks-->
	<append xpath="/blocks">
		<block name="x51FarmPlotVoxelBlock">
			<property name="Material" value="mFarmplot_Voxel_Block"/>
			<property name="Shape" value="New"/>
			<property name="Model" value="farm_plot"/>
			<property name="Texture" value="65"/>
			<property name="CustomIcon" value="farmPlotBlock"/>
			<property name="CustomIconTint" value="C4A484"/>
			<property name="UiBackgroundTexture" value="65"/>
			<property name="ItemTypeIcon" value="frames"/>
			<property name="ItemTypeIconColor" value="128,256,128,255" />
			<property name="CreativeMode" value="Dev"/>
			<property name="Group" value="Food/Cooking,Building,advBuilding"/>
			<property name="FilterTags" value="MC_outdoor,SC_crops"/>
			<property name="Tags" value="earth"/>
			<property name="DescriptionKey" value="x51FarmPlotVoxelBlockDesc"/>
			<property name="FuelValue" value="0"/>
			<property class="RepairItems">
				<property name="resourceClayLump" value="3"/>
			</property>
			<drop event="Destroy" name="x51FarmPlotVoxelBlockVariantHelper"/>
			<property name="EconomicValue" value="10"/>
			<property name="EconomicBundleSize" value="20"/>
			<property name="SortOrder1" value="B965"/>
			<property name="SortOrder2" value="0000"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" value="x51FarmPlotVoxel"/>
				<property name="Item" value="resourceWood"/>
				<property name="ItemCount" value="0"/>
				<property name="UpgradeHitCount" value="1"/>
			</property>
			<property name="PassThroughDamage" value="true"/>
			<property name="CustomUpgradeSound" value="place_block_wood"/>
			<property name="CanPickup" value="true"/>
		</block>
	</append>

	<append xpath="/blocks">
